{
  "version": 2,
  "builds": [
   {
    "src": "apis/server.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["dist/**"] }
   }
  ],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "apis/server.js"
   }
  ],"rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
 }